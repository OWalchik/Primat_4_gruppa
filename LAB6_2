
#include <iostream>

int main()
{
    unsigned int N;
    unsigned int mult = 1;
    unsigned short n;
    std::cin >> N;

    while (N != 0)
    {
        n = N % 10;
        
        if (n % 2 != 0)
        {
            mult *= n;
        }

        N /= 10;
    }
    
    std::cout << mult;

    return 1;
}
